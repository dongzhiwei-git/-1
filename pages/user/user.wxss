
.page{
  width: 100%;
  height: 100%;
  background-color: #ffffff73;
}
.Login{
  background-color: #f2b48a;
  border-bottom: 1px solid #f2b48a;
  border-bottom-left-radius: 13rpx;
  
  border-bottom-right-radius: 13rpx;
  height: 30vh;

}
.no_login{
  padding-top: 100rpx;

}

.user_{
  height: 30vh;
  overflow: hidden;
  background-color: var(--themeColor);
  position: relative;
  border-bottom: 1px solid #f2b48a;
  border-bottom-left-radius: 13rpx;
  
  border-bottom-right-radius: 13rpx;
  box-shadow: rgb(107, 105, 105) 3rpx 3rpx;
  /* box-shadow: black, 12rpx;
   */
  
}


.user_message{
  height: 100%;
  /* position: absolute ; */
  top: 0vh;
}

.user_msg{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 30rpx auto;
  padding: 10rpx;
}
.other-navigator-hover{
  color: #000000;
}
.user_icon{
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  /* margin-left: 100rpx;
  margin-right: 30rpx;
  margin-top: -80rpx; */
}
.user_text{
  height: 100%;
  display: flex;
  margin-top: 30rpx;
  padding-left: auto 20rpx;
  /* align-items: center; */
  flex-direction: column;
  font-size: 35rpx;
  line-height: 1.5rem;
  letter-spacing: 7rpx;
}
.user_name{
  letter-spacing: 3rpx;
  margin-bottom: 30rpx;
  color: rgb(14, 134, 204);
}

